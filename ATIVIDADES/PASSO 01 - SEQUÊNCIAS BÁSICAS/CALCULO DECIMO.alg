algoritmo "CALCULO 13"
// Seção de Declarações
var

mesesTrabalhados: inteiro
v1,Vsalario, vdecimo: real

inicio

escreval ("Digite o seu salario: ")
leia (Vsalario)
escreval ("Digite os meses trabalhados na empresa: ")
leia (mesesTrabalhados)

v1 <- Vsalario/12
vdecimo <- mesesTrabalhados*v1


escreval(" O valor total de seu décimo é : ", vdecimo)


fimalgoritmo
